{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "connect-src 'self' https://study-portal-ill8.onrender.com https://study-portal-qitc.vercel.app"
        }
      ]
    }
  ]
}